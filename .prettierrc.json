{
  "semi": true,
  "trailingComma": "es5",
  "singleQuote": true,
  "printWidth": 80,
  "tabWidth": 2,
  "importOrder": [
    "^@kartuli/ui/src/theme/unistyles",
    "^[.][.]/wdyr$",
    "^react$",
    "^react-native$",
    "<THIRD_PARTY_MODULES>",
    "^@kartuli/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
